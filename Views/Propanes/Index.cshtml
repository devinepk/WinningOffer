@model IEnumerable<WinningOffer.Models.Propane>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Owned)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Leased)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WillRemain)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NotRemain)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NotApplication)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Owned)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Leased)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WillRemain)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NotRemain)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NotApplication)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
